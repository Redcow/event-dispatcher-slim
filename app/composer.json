{
    "name": "fplet/localfood",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Infrastructure\\": "src/infrastructure/",
            "Domain\\" : "src/domain/",
            "Bootstrap\\": "src/bootstrap/"
        },
        "files": [
            "./src/bootstrap/routes.php",
            "./src/bootstrap/listeners.php",
            "./src/bootstrap/definitions.php"
        ]
    },
    "authors": [
        {
            "name": "Francois Plet"
        }
    ],
    "require": {
        "slim/slim": "^4.12",
        "php-di/php-di": "^7.0",
        "slim/psr7": "^1.6",
        "php-di/slim-bridge": "^3.4",
        "ext-xdebug": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "behat/behat": "^3.13",
        "phpunit/phpunit": "^10.4"
    }
}
